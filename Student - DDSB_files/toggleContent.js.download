        $("#contactMainContainer").toggleClass("hide", $(this).is(":checked"));
        $("#hdnHideRightContact").val($("#contactMainContainer").hasClass("hide") ? "hide" : "");
        if (iCreateObject.isSiteInICreateMode) $("#TKInteriorHideAreasToggle").save();

        checkFloatRight();
